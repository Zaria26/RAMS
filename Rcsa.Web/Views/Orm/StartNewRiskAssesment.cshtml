@model Rcsa.Web.Models.RiskAssessmentMaster

@{
    Layout = null;
}
<html>
<head>
    <title>Start New Risk Assesment</title>
</head>
<body>
    @using (Html.BeginForm())
    {
        @Html.ValidationSummary(true)    
        <div>
            <div>
                <table>
                    <tr>
                        <td>Risk Assesment Name</td>
                        <td>@Html.TextBoxFor(model => model.RiskAssessmentName)</td>
                    </tr>
                </table>
            </div>
        </div>     
    }

</body>
</html>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}